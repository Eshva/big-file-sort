    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <Copyright>Copyright (c) 2022 Mike Eshva</Copyright>
    <Company>Mike Eshva</Company>
    <Version>1.1.1</Version>
